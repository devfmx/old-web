<!DOCTYPE html>
<html>
<head>
  <title>Dev.f</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
  <%= yield :javascripts %>
  <%= csrf_meta_tags %>
  <%= render :partial => 'layouts/google_tracking' %>
</head>
<body>
  <header class="main-header">
    <div class="wrapper">
      <div class="col-12">
        <div class="col col-12">
          <div class="col-inner">
            <%= link_to root_path, class: 'devf-logo-head' do %>
              <i class='devf-logo'></i>
              <span class="tagline"><%= Settings.tagline %></span>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </header>
  <%= yield %>
</body>
</html>
